{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "@aspnet/signalr@1.0.27",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "crypto-js@4.1.1",
      "destination": "wwwroot/lib/crypto-js/",
      "files": [
        "crypto-js.min.js",
        "aes.min.js"
      ]
    }
  ]
}